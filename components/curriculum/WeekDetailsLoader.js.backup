/**\n * COMPONENTE WeekDetailsLoader - MISI√ìN 183.2\n * \n * Maneja el lazy loading de datos de detalle de semana con estados:\n * - Carga (loading)\n * - Error con retry\n * - Datos completos (render WeekDetails)\n * \n * @author Mentor Coder\n * @version v1.0\n * @arquitectura Lazy Loading Pattern\n */\n\nimport WeekDetails from './WeekDetails';\nimport { ExclamationTriangleIcon, ArrowPathIcon } from '@heroicons/react/24/outline';\n\nexport default function WeekDetailsLoader({\n  activeWeek,\n  weekDetailsData,\n  loadingWeekDetails,\n  weekDetailsError,\n  onRetry\n}) {\n  // Estado de carga\n  if (loadingWeekDetails) {\n    return (\n      <div className=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n        <div className=\"bg-gradient-to-r from-blue-50 to-indigo-50 px-6 py-8\">\n          <div className=\"text-center\">\n            {/* Indicador de carga animado */}\n            <div className=\"flex justify-center mb-4\">\n              <div className=\"animate-spin rounded-full h-12 w-12 border-b-2 border-indigo-600\"></div>\n            </div>\n            \n            {/* Informaci√≥n de la semana que se est√° cargando */}\n            <h3 className=\"text-xl font-semibold text-gray-900 mb-2\">\n              Cargando detalles de la Semana {activeWeek.semana}\n            </h3>\n            <h4 className=\"text-lg text-indigo-700 mb-1\">\n              {activeWeek.tituloSemana}\n            </h4>\n            <p className=\"text-gray-600 text-sm\">\n              Obteniendo esquema diario, objetivos y recursos...\n            </p>\n            \n            {/* Barra de progreso simulada */}\n            <div className=\"mt-6 max-w-md mx-auto\">\n              <div className=\"bg-gray-200 rounded-full h-2\">\n                <div className=\"bg-indigo-600 h-2 rounded-full animate-pulse\" style={{width: '70%'}}></div>\n              </div>\n              <p className=\"text-xs text-gray-500 mt-2\">\n                Cargando datos desde la base de datos...\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n\n  // Estado de error\n  if (weekDetailsError) {\n    return (\n      <div className=\"bg-white rounded-xl shadow-lg border border-red-200 overflow-hidden\">\n        <div className=\"bg-gradient-to-r from-red-50 to-orange-50 px-6 py-8\">\n          <div className=\"text-center\">\n            {/* Icono de error */}\n            <div className=\"flex justify-center mb-4\">\n              <ExclamationTriangleIcon className=\"h-12 w-12 text-red-500\" />\n            </div>\n            \n            {/* Informaci√≥n del error */}\n            <h3 className=\"text-xl font-semibold text-red-900 mb-2\">\n              Error al cargar los detalles\n            </h3>\n            <h4 className=\"text-lg text-gray-700 mb-2\">\n              Semana {weekDetailsError.weekId}: {activeWeek.tituloSemana}\n            </h4>\n            <p className=\"text-red-600 text-sm mb-1\">\n              {weekDetailsError.message}\n            </p>\n            <p className=\"text-red-500 text-xs mb-6\">\n              {weekDetailsError.details}\n            </p>\n            \n            {/* Bot√≥n de retry */}\n            <button\n              onClick={onRetry}\n              className=\"inline-flex items-center px-4 py-2 bg-red-600 text-white rounded-lg hover:bg-red-700 transition-colors duration-200\"\n            >\n              <ArrowPathIcon className=\"w-4 h-4 mr-2\" />\n              Reintentar carga\n            </button>\n            \n            {/* Sugerencias de troubleshooting */}\n            <div className=\"mt-6 text-left max-w-md mx-auto\">\n              <p className=\"text-xs text-gray-600 mb-2\">üí° <strong>Posibles soluciones:</strong></p>\n              <ul className=\"text-xs text-gray-500 space-y-1\">\n                <li>‚Ä¢ Verificar que el servidor est√© ejecut√°ndose</li>\n                <li>‚Ä¢ Comprobar la conexi√≥n de red</li>\n                <li>‚Ä¢ Revisar que la base de datos est√© disponible</li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n\n  // Estado con datos completos - renderizar WeekDetails\n  if (weekDetailsData) {\n    return (\n      <div className=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n        {/* Header indicando que los datos fueron cargados exitosamente */}\n        <div className=\"bg-gradient-to-r from-green-50 to-emerald-50 px-6 py-3 border-b border-green-100\">\n          <div className=\"flex items-center justify-between\">\n            <div className=\"flex items-center space-x-3\">\n              <div className=\"w-3 h-3 bg-green-500 rounded-full animate-pulse\"></div>\n              <span className=\"text-green-800 text-sm font-medium\">\n                üìä Datos completos cargados desde SQLite\n              </span>\n            </div>\n            <div className=\"text-xs text-green-600\">\n              Esquema diario: {weekDetailsData.esquemaDiario?.length || 0} d√≠as\n            </div>\n          </div>\n        </div>\n        \n        {/* Componente WeekDetails con datos completos */}\n        <WeekDetails weekData={weekDetailsData} />\n      </div>\n    );\n  }\n\n  // Estado por defecto (no deber√≠a ocurrir)\n  return (\n    <div className=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n      <div className=\"px-6 py-8 text-center\">\n        <p className=\"text-gray-500\">\n          No hay datos de semana para mostrar\n        </p>\n      </div>\n    </div>\n  );\n}","newText":"/**\n * COMPONENTE WeekDetailsLoader - MISI√ìN 183.2\n * \n * Maneja el lazy loading de datos de detalle de semana con estados:\n * - Carga (loading)\n * - Error con retry\n * - Datos completos (render WeekDetails)\n * \n * @author Mentor Coder\n * @version v1.0\n * @arquitectura Lazy Loading Pattern\n */\n\nimport WeekDetails from './WeekDetails';\nimport { ExclamationTriangleIcon, ArrowPathIcon } from '@heroicons/react/24/outline';\n\nexport default function WeekDetailsLoader({\n  activeWeek,\n  weekDetailsData,\n  loadingWeekDetails,\n  weekDetailsError,\n  onRetry\n}) {\n  // Estado de carga\n  if (loadingWeekDetails) {\n    return (\n      <div className=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n        <div className=\"bg-gradient-to-r from-blue-50 to-indigo-50 px-6 py-8\">\n          <div className=\"text-center\">\n            {/* Indicador de carga animado */}\n            <div className=\"flex justify-center mb-4\">\n              <div className=\"animate-spin rounded-full h-12 w-12 border-b-2 border-indigo-600\"></div>\n            </div>\n            \n            {/* Informaci√≥n de la semana que se est√° cargando */}\n            <h3 className=\"text-xl font-semibold text-gray-900 mb-2\">\n              Cargando detalles de la Semana {activeWeek.semana}\n            </h3>\n            <h4 className=\"text-lg text-indigo-700 mb-1\">\n              {activeWeek.tituloSemana}\n            </h4>\n            <p className=\"text-gray-600 text-sm\">\n              Obteniendo esquema diario, objetivos y recursos...\n            </p>\n            \n            {/* Barra de progreso simulada */}\n            <div className=\"mt-6 max-w-md mx-auto\">\n              <div className=\"bg-gray-200 rounded-full h-2\">\n                <div className=\"bg-indigo-600 h-2 rounded-full animate-pulse\" style={{width: '70%'}}></div>\n              </div>\n              <p className=\"text-xs text-gray-500 mt-2\">\n                Cargando datos desde la base de datos...\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n\n  // Estado de error\n  if (weekDetailsError) {\n    return (\n      <div className=\"bg-white rounded-xl shadow-lg border border-red-200 overflow-hidden\">\n        <div className=\"bg-gradient-to-r from-red-50 to-orange-50 px-6 py-8\">\n          <div className=\"text-center\">\n            {/* Icono de error */}\n            <div className=\"flex justify-center mb-4\">\n              <ExclamationTriangleIcon className=\"h-12 w-12 text-red-500\" />\n            </div>\n            \n            {/* Informaci√≥n del error */}\n            <h3 className=\"text-xl font-semibold text-red-900 mb-2\">\n              Error al cargar los detalles\n            </h3>\n            <h4 className=\"text-lg text-gray-700 mb-2\">\n              Semana {weekDetailsError.weekId}: {activeWeek.tituloSemana}\n            </h4>\n            <p className=\"text-red-600 text-sm mb-1\">\n              {weekDetailsError.message}\n            </p>\n            <p className=\"text-red-500 text-xs mb-6\">\n              {weekDetailsError.details}\n            </p>\n            \n            {/* Bot√≥n de retry */}\n            <button\n              onClick={onRetry}\n              className=\"inline-flex items-center px-4 py-2 bg-red-600 text-white rounded-lg hover:bg-red-700 transition-colors duration-200\"\n            >\n              <ArrowPathIcon className=\"w-4 h-4 mr-2\" />\n              Reintentar carga\n            </button>\n            \n            {/* Sugerencias de troubleshooting */}\n            <div className=\"mt-6 text-left max-w-md mx-auto\">\n              <p className=\"text-xs text-gray-600 mb-2\">üí° <strong>Posibles soluciones:</strong></p>\n              <ul className=\"text-xs text-gray-500 space-y-1\">\n                <li>‚Ä¢ Verificar que el servidor est√© ejecut√°ndose</li>\n                <li>‚Ä¢ Comprobar la conexi√≥n de red</li>\n                <li>‚Ä¢ Revisar que la base de datos est√© disponible</li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n\n  // Estado con datos completos - renderizar WeekDetails\n  if (weekDetailsData) {\n    return (\n      <div className=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n        {/* Header indicando que los datos fueron cargados exitosamente */}\n        <div className=\"bg-gradient-to-r from-green-50 to-emerald-50 px-6 py-3 border-b border-green-100\">\n          <div className=\"flex items-center justify-between\">\n            <div className=\"flex items-center space-x-3\">\n              <div className=\"w-3 h-3 bg-green-500 rounded-full animate-pulse\"></div>\n              <span className=\"text-green-800 text-sm font-medium\">\n                üìä Datos completos cargados desde SQLite\n              </span>\n            </div>\n            <div className=\"text-xs text-green-600\">\n              Esquema diario: {weekDetailsData.esquemaDiario?.length || 0} d√≠as\n            </div>\n          </div>\n        </div>\n        \n        {/* Componente WeekDetails con datos completos */}\n        <WeekDetails weekData={weekDetailsData} />\n      </div>\n    );\n  }\n\n  // Estado por defecto (no deber√≠a ocurrir)\n  return (\n    <div className=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n      <div className=\"px-6 py-8 text-center\">\n        <p className=\"text-gray-500\">\n          No hay datos de semana para mostrar\n        </p>\n      </div>\n    </div>\n  );\n}"}]